<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"> 	<xsl:template match="/data">		<xsl:variable name="pics" select="*[@data-cms-name='pics']"/>		<!--		<xsl:variable name="icons" select="*[@data-cms-name='icons']"/>		<xsl:variable name="disk" select="$icons[@path = 'disk.small.png']"/>		-->		<div class="minecraft-pics">			<section class="paintedBox">				<h2 data-dict="">pics/upload</h2>				<form action="." method="POST" enctype="multipart/form-data">					<input type="file" name="pic[]" multiple="multiple" accept="image/*"/>					<input type="submit" value="pics/submit" data-dict="@value"/>				</form>			</section>			<section>				<h2 data-dict="">pics/uploaded</h2>				<ul class="minecraft-pic-preview">					<xsl:for-each select="$pics">						<xsl:sort select="@change-stamp" order="descending"/>						<li>							<a href="{@uri}" title="{@path}">								<!--								<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">									<image xlink:href="{@thumbnail}" width="200" height="200" preserveAspectRatio="xMinYMin slice"/>								</svg>								-->								<img src="{@thumbnail}" alt="{@path}" />								<div/>							</a>							<p>								<xsl:value-of select="@path"/>							</p>							<p>								<time datetime="{@change-utc}" title="Erstellungsdatum">(<xsl:value-of select="@change-datetime"/>)</time>							</p>							<samp>								<xsl:value-of select="ceiling(@size div 1024 div 1024)"/>								<xsl:text> MB </xsl:text>								<a href="{@uri}?download" title="Download!" class="downloadFile" download="{@path}">									<!--<img src="data:{$disk/@type};base64,{$disk/@base64}" alt="Download"/>-->									<xsl:text>ðŸ’¾</xsl:text>								</a>								<xsl:text> </xsl:text>								<xsl:value-of select="@width"/>								<xsl:text> x </xsl:text>								<xsl:value-of select="@height"/>								<xsl:text> Pixel</xsl:text>							</samp>							<!--							<p>								<xsl:value-of select="@path"/>								<a href="/download.php?file={@uri}" title="Download Pic" class="downloadFile">									<img src="/images/pictograms/disk.default.png" alt="Download"/>													</a>								<xsl:text> (</xsl:text>								<time datetime="{@change-utc}" title="Erstellungsdatum"><xsl:value-of select="@change-datetime"/></time>								<xsl:text>)</xsl:text>							</p>							-->						</li>					</xsl:for-each>				</ul>			</section>		</div>	</xsl:template></xsl:stylesheet>