<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:template match="/data">		<xsl:variable name="maps"  select="*[@data-cms-name='maps']"/>		<!--		<xsl:variable name="icons" select="*[@data-cms-name='icons']"/>		<xsl:variable name="disk" select="$icons[@path = 'disk.small.png']"/>		-->		<section>			<h2>Current and Previous World Maps</h2>			<ul class="minecraft-pic-preview">				<xsl:for-each select="$maps">					<xsl:sort select="@change-stamp" order="descending"/>					<li>						<a href="{@uri}" title="{@path}">							<img src="{@thumbnail}" alt="{@path}" />							<div/>						</a>						<p>							<xsl:value-of select="@path"/>						</p>						<p>							<time datetime="{@change-utc}" title="Erstellungsdatum">(<xsl:value-of select="@change-datetime"/>)</time>						</p>						<samp>							<xsl:value-of select="ceiling(@size div 1024 div 1024)"/>							<xsl:text> MB </xsl:text>							<a href="{@uri}?download" title="Download!" class="downloadFile" download="{@path}">								<!--<img src="data:{$disk/@type};base64,{$disk/@base64}" alt="Download"/>-->								<xsl:text>ðŸ’¾</xsl:text>							</a>							<xsl:text> </xsl:text>							<xsl:value-of select="@width"/>							<xsl:text> x </xsl:text>							<xsl:value-of select="@height"/>							<xsl:text> Pixel</xsl:text>						</samp>					</li>				</xsl:for-each>			</ul>		</section>	</xsl:template></xsl:stylesheet>