<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:import href="/getTemplate.php/minecraft/log"/>	<xsl:import href="/getTemplate.php/minecraft/functions"/>	<xsl:template match="/data">		<div class="minecraft-infos" data-dict=".//html:h2/text() | .//html:th/text()">			<xsl:for-each select="*[@data-cms-name='server']">				<article>					<h2>Minecraft Einstellungen</h2>					<xsl:apply-templates select="."/>				</article>			</xsl:for-each>			<xsl:for-each select="*[@data-cms-name='hardware']">				<article>					<h2>Server Hardware</h2>					<xsl:apply-templates select="."/>				</article>			</xsl:for-each>			<xsl:for-each select="*[@data-cms-name='info']">				<article>					<h2>Welt Statistik</h2>					<xsl:apply-templates select="."/>				</article>			</xsl:for-each>		</div>	</xsl:template>	<xsl:template match="*[@type='text/csv']">		<table class="paintedTable">			<thead>				<tr>					<th>Schl√ºssel</th>					<th>Wert</th>				</tr>			</thead>			<tbody>				<xsl:for-each select="line[not(contains(@key, 'password'))]">					<xsl:sort select="@key"/>					<tr>						<td><xsl:value-of select="@key"/></td>						<td><xsl:value-of select="@val"/></td>					</tr>				</xsl:for-each>			</tbody>		</table>	</xsl:template>	<xsl:template match="*[@data-cms-name='info']">		<xsl:variable name="mapping" select="/data/*[@data-cms-name='mapping']/object/*"/>		<table class="paintedTable">			<thead>				<tr>					<th>Anzahl</th>					<th colspan="2">Block</th>				</tr>			</thead>			<tbody>				<xsl:for-each select="line[string-length(@val) &gt; 0]">					<xsl:variable name="name" select="@key"/>					<xsl:variable name="map" select="$mapping[@key=$name]"/>					<tr>						<td class="number"><xsl:value-of select="@val"/></td>						<td>							<xsl:if test="$map">								<xsl:call-template name="block-image">									<xsl:with-param name="id" select="$map/@val"/>								</xsl:call-template>							</xsl:if>						</td>						<td data-dict=""><xsl:value-of select="string(@key)"/></td>					</tr>				</xsl:for-each>			</tbody>		</table>	</xsl:template></xsl:stylesheet>