<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:template match="/data">		<div>			<xsl:for-each select="*[@data-cms-name='news']//news">				<article>					<h2>						<xsl:value-of select="@title"/>						<small class="number">							<xsl:text>&#160;(</xsl:text>							<xsl:value-of select="@date"/>							<xsl:text>)</xsl:text>						</small>					</h2>					<xsl:copy-of select="*"/>				</article>				<xsl:if test="position() != last()">					<hr/>				</xsl:if>			</xsl:for-each>		</div>	</xsl:template></xsl:stylesheet>