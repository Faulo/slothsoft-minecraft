<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0"	xmlns="http://www.w3.org/1999/xhtml"	xmlns:xsl="http://www.w3.org/1999/XSL/Transform"> 	<xsl:output method="xml" indent="no" encoding="UTF-8" media-type="application/xhtml+xml" version="1.0"/>	<xsl:template match="/data">		<xsl:variable name="pageTitles" select="request/*"/>		<xsl:variable name="requestedPage" select="request/*[1]"/>		<html>			<head>				<title>					<!--<xsl:value-of select="page/@header"/>-->					<xsl:for-each select="$pageTitles">						<xsl:if test="position() &gt; 1">							<xsl:text> - </xsl:text>						</xsl:if>						<p data-dict="."><xsl:value-of select="@title"/></p>					</xsl:for-each>				</title>				<link rel="icon" type="image/png" href="/getResource.php/slothsoft/favicon"/>				<meta name="author" content="Daniel Schulz"/>				<meta name="keywords">					<xsl:attribute name="content">						<xsl:for-each select="$pageTitles">							<xsl:if test="position() &gt; 1">								<xsl:text>,</xsl:text>							</xsl:if>							<p data-dict="."><xsl:value-of select="@title"/></p>						</xsl:for-each>					</xsl:attribute>				</meta>				<!--<xsl:copy-of select="."/>-->			</head>			<body>				<div>					<nav class="Navi Map" style="margin: 1em">						<xsl:copy-of select="*[@data-cms-name='navi']/node()"/>						<xsl:copy-of select="*[@data-cms-name='dict.select']/node()"/>						<xsl:copy-of select="*[@data-cms-name='validator']/node()"/>					</nav>					<img class="Content Map" src="{*[@data-cms-name='map']/@uri}" alt="Weltkarte" />									</div>			</body>		</html>	</xsl:template></xsl:stylesheet>